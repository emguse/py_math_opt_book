   "source": [
    "from mip import Model, maximize\n",
    "from mip_tool.view import view_model\n",
    "\n",
    "m = Model()\n",
    "x = m.add_var(\"x\")\n",
    "y = m.add_var(\"y\")\n",
    "m.objective = maximize(2 * x + y)\n",
    "m += x + y <= 2\n",
    "m += x <= y\n",
    "view_model(m)"
   ]
  }
 ],
